{
  "manifest_version": 3,
  "name": "iidx rlt",
  "version": "1.0.1",
  "permissions": ["activeTab", "storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://p.eagate.573.jp/game/2dx/*/djdata/random_lane/*"],
      "js": ["src/content.ts"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://p.eagate.573.jp/*"],
      "resources": ["assets/*", "vendor/*.js"]
    }
  ]
}
